public class min {
    public static void main(String[] args) {
        int arr[] = {1, 2, 3, 4, 5, 6, 7, 8};
        
      
        int min = arr[0];

     
        for (int i = 1; i < arr.length; i++) {
            if (arr[i] < min) {
                min = arr[i];
            }
        }

        // Print the minimum value after the loop ends
        System.out.println("Minimum value: " + min);
    }
}
